                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
    p.ProductID,
    p.Name AS ProductName,
    pc.Name AS CategoryName,
    psc.Name AS SubcategoryName,
    p.Color,
    p.Size,
    pm.Name AS BrandName,
    p.StandardCost,
    
    ISNULL((
        SELECT TOP 1 l.ListPrice
        FROM CompanyX.Production.ProductListPriceHistory l
        WHERE l.ProductID = p.ProductID
          AND GETDATE() BETWEEN l.StartDate AND ISNULL(l.EndDate,'9999-12-31')
        ORDER BY l.StartDate DESC
    ), p.ListPrice) AS ListPrice,

    
    CONVERT(VARCHAR(64),
        HASHBYTES('SHA2_256',
            CONCAT(
                -- Chuẩn hóa và nối các cột chuỗi
                UPPER(TRIM(p.Name)),'|', 
                UPPER(TRIM(pc.Name)),'|', 
                UPPER(TRIM(psc.Name)),'|',
                ISNULL(UPPER(TRIM(p.Color)),''),'|', 
                ISNULL(UPPER(TRIM(p.Size)),''),'|',
                ISNULL(UPPER(TRIM(pm.Name)),''),'|',
                p.StandardCost,'|', 
                p.ListPrice 
            )
        ), 2
    ) AS RowHash
FROM 
    CompanyX.Production.Product p
LEFT JOIN 
    CompanyX.Production.ProductSubcategory psc ON p.ProductSubcategoryID = psc.ProductSubcategoryID
LEFT JOIN 
    CompanyX.Production.ProductCategory    pc  ON psc.ProductCategoryID = pc.ProductCategoryID
LEFT JOIN 
    CompanyX.Production.ProductModel       pm  ON pm.ProductModelID = p.ProductModelID
WHERE 
    p.FinishedGoodsFlag = 1;</property>
