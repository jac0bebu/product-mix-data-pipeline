          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{582764F9-1437-45D3-99D4-E1BEDBBCD247}"
              SQLTask:SqlStatementSource="&#xA;UPDATE f&#xA;SET&#xA;    -- Lấy Key (ví dụ: 1, 2, 3) từ bảng DimSeasonality&#xA;    f.SeasonalityKey = ds.SeasonalityKey&#xA;FROM&#xA;    dbo.FactProductMix AS f&#xA;JOIN&#xA;    dbo.DimSeasonality AS ds&#xA;    -- Nối 2 bảng bằng cách xem Index có nằm GIỮA (BETWEEN)&#xA;    -- khoảng Lower và Upper không&#xA;    ON f.SeasonalityIndex BETWEEN ds.IndexLowerBound AND ds.IndexUpperBound&#xA;WHERE&#xA;    f.SeasonalityIndex IS NOT NULL -- Chỉ cập nhật những hàng đã tính được Index&#xA;    AND (f.SeasonalityKey IS NULL OR f.SeasonalityKey &lt;&gt; ds.SeasonalityKey); -- Tối ưu: Chỉ cập nhật nếu Key bị NULL hoặc khác&#xA;&#xA;PRINT 'Hoàn tất cập nhật SeasonalityKey.';" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
          </DTS:ObjectData>
        </DTS:Executable>
