          <DTS:Variables />
          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{582764F9-1437-45D3-99D4-E1BEDBBCD247}"
              SQLTask:SqlStatementSource="-- CTE để đếm số danh mục duy nhất trong mỗi Market/Date&#xA;WITH CatCounts AS (&#xA;    SELECT&#xA;        d.MarketKey,&#xA;        d.DateKey,&#xA;        COUNT(*) AS CategoryCount&#xA;    FROM&#xA;        (SELECT DISTINCT&#xA;            f.MarketKey,&#xA;            f.DateKey,&#xA;            p.CategoryName&#xA;        FROM dbo.FactProductMix f&#xA;        JOIN dbo.DimProduct p ON p.ProductKey = f.ProductKey) d&#xA;    GROUP BY&#xA;        d.MarketKey,&#xA;        d.DateKey&#xA;),&#xA;-- CTE để đếm số SKU duy nhất trong mỗi Market/Date&#xA;SKUCounts AS (&#xA;    SELECT&#xA;        d.MarketKey,&#xA;        d.DateKey,&#xA;        COUNT(*) AS SKUCount&#xA;    FROM&#xA;        (SELECT DISTINCT&#xA;            f.MarketKey,&#xA;            f.DateKey,&#xA;            f.ProductKey&#xA;        FROM dbo.FactProductMix f) d&#xA;    GROUP BY&#xA;        d.MarketKey,&#xA;        d.DateKey&#xA;),&#xA;-- CTE để đếm số biến thể (variants) dựa trên BrandName&#xA;VariantCounts AS (&#xA;    SELECT&#xA;        BrandName,&#xA;        COUNT(ProductKey) AS TrueVariantCount&#xA;    FROM&#xA;        dbo.DimProduct&#xA;    GROUP BY&#xA;        BrandName&#xA;),&#xA;-- CTE tính doanh thu trung bình cho Seasonality Index (theo Market)&#xA;MarketProductAverages AS (&#xA;    SELECT&#xA;        ProductKey,&#xA;        MarketKey,&#xA;        AVG(TotalRevenue) AS AvgMarketMonthlyRevenue&#xA;    FROM&#xA;        dbo.FactProductMix&#xA;    WHERE&#xA;        TotalRevenue &gt; 0&#xA;    GROUP BY&#xA;        ProductKey,&#xA;        MarketKey&#xA;),&#xA;-- === CTE MỚI: Tính AVG và STDEV cho Consistency Index (CV) (theo Product chung) ===&#xA;ProductStats AS (&#xA;    SELECT&#xA;        ProductKey,&#xA;        AVG(TotalRevenue) AS AvgRevenue, -- Trung bình doanh thu toàn lịch sử của sản phẩm&#xA;        STDEV(TotalRevenue) AS StdevRevenue -- Độ lệch chuẩn doanh thu toàn lịch sử&#xA;    FROM&#xA;        dbo.FactProductMix&#xA;    WHERE&#xA;        TotalRevenue &gt; 0&#xA;    GROUP BY&#xA;        ProductKey&#xA;    HAVING&#xA;        COUNT(*) &gt; 1 -- Cần ít nhất 2 điểm dữ liệu (tháng) để tính STDEV&#xA;)&#xA;-- =========================================================&#xA;-- UPDATE bảng fact với tất cả các chỉ số&#xA;-- =========================================================&#xA;UPDATE f&#xA;SET&#xA;    f.CategoryCount = cc.CategoryCount,&#xA;    f.SKUCount      = sc.SKUCount,&#xA;    f.VariantCount  = vc.TrueVariantCount,&#xA;    -- Tính Seasonality Index&#xA;    f.SeasonalityIndex = f.TotalRevenue / NULLIF(mpa.AvgMarketMonthlyRevenue, 0),&#xA;    -- === THÊM CÔNG THỨC Consistency Index (CV) VÀO ĐÂY ===&#xA;    f.ConsistencyIndex = ps.StdevRevenue / NULLIF(ps.AvgRevenue, 0)&#xA;FROM&#xA;    dbo.FactProductMix f&#xA;-- Join DimProduct để lấy BrandName cho VariantCount&#xA;JOIN dbo.DimProduct p ON p.ProductKey = f.ProductKey&#xA;-- Join các CTE đếm Category và SKU&#xA;JOIN CatCounts cc ON cc.MarketKey = f.MarketKey AND cc.DateKey = f.DateKey&#xA;JOIN SKUCounts sc ON sc.MarketKey = f.MarketKey AND sc.DateKey = f.DateKey&#xA;-- Join CTE đếm Variant dựa trên BrandName&#xA;JOIN VariantCounts vc ON vc.BrandName = p.BrandName&#xA;-- Join CTE để lấy doanh thu trung bình (theo Market) cho Seasonality Index&#xA;JOIN MarketProductAverages mpa ON f.ProductKey = mpa.ProductKey AND f.MarketKey = mpa.MarketKey&#xA;-- === JOIN CTE MỚI để lấy AVG và STDEV cho Consistency Index (CV) ===&#xA;-- Dùng LEFT JOIN phòng trường hợp sản phẩm chỉ có 1 tháng dữ liệu (không tính được STDEV)&#xA;LEFT JOIN ProductStats ps ON f.ProductKey = ps.ProductKey;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
          </DTS:ObjectData>
        </DTS:Executable>
