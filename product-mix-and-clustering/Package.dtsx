                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH S AS (
  SELECT
    soh.TerritoryID,
    CAST(soh.OrderDate AS DATE) AS OrderDate,
    sod.ProductID,
    SUM(sod.LineTotal) AS TotalRevenue,
    SUM(sod.OrderQty)  AS TotalQuantity,
    SUM((sod.UnitPrice - p.StandardCost) * sod.OrderQty) AS TotalProfit
  FROM Sales.SalesOrderHeader soh
  JOIN Sales.SalesOrderDetail sod ON sod.SalesOrderID = soh.SalesOrderID
  JOIN Production.Product p ON p.ProductID = sod.ProductID
  WHERE soh.OrderDate &gt; ?                  -- watermark from Fact
  GROUP BY soh.TerritoryID, CAST(soh.OrderDate AS DATE), sod.ProductID
),
J AS (
  -- resolve DWH surrogate keys and attributes (true SCD2 alignment)
  SELECT
    dp.ProductKey,
    dm.MarketKey,
    dd.DateKey,
    dp.ProductID,
    dp.CategoryName,
    dp.Size,
    S.TotalRevenue,
    S.TotalQuantity,
    S.TotalProfit
  FROM S
  JOIN ProductMixDWH.dbo.DimDate   dd ON dd.DateKey = S.OrderDate
  JOIN ProductMixDWH.dbo.DimMarket dm ON dm.MarketID = S.TerritoryID
  JOIN ProductMixDWH.dbo.DimProduct dp
       ON dp.ProductID = S.ProductID
      AND S.OrderDate BETWEEN dp.ValidFrom AND dp.ValidTo
),
-- DISTINCT counts done via grouped CTEs (NO window DISTINCT!)
CatCounts AS (
  SELECT MarketKey, DateKey, COUNT(*) AS CategoryCount
  FROM (
    SELECT DISTINCT MarketKey, DateKey, CategoryName
    FROM J
  ) d
  GROUP BY MarketKey, DateKey
),
SKUCounts AS (
  SELECT MarketKey, DateKey, COUNT(*) AS SKUCount
  FROM (
    SELECT DISTINCT MarketKey, DateKey, ProductKey
    FROM J
  ) d
  GROUP BY MarketKey, DateKey
),
Variants AS (
  SELECT MarketKey, DateKey, ProductID, Size, COUNT(*) AS VariantCount
  FROM J
  GROUP BY MarketKey, DateKey, ProductID, Size
)
SELECT
  J.ProductKey,
  J.MarketKey,
  J.DateKey,
  J.ProductID,
  J.TotalRevenue,
  J.TotalProfit,
  J.TotalQuantity,
  CC.CategoryCount,
  SK.SKUCount,
  V.VariantCount
FROM J
JOIN CatCounts CC
  ON CC.MarketKey = J.MarketKey AND CC.DateKey = J.DateKey
JOIN SKUCounts SK
  ON SK.MarketKey = J.MarketKey AND SK.DateKey = J.DateKey
JOIN Variants V
  ON V.MarketKey = J.MarketKey
 AND V.DateKey   = J.DateKey
 AND V.ProductID = J.ProductID
 AND ISNULL(V.Size, '') = ISNULL(J.Size, '');</property>
