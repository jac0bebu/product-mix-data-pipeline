                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
  p.ProductID,
  p.Name AS ProductName,
  pc.Name AS CategoryName,
  psc.Name AS SubcategoryName,
  p.Color,
  p.Size,
  pm.Name AS BrandName,
  p.StandardCost,
  -- Get current list price (fallback to Product.ListPrice)
  ISNULL((
     SELECT TOP 1 l.ListPrice
     FROM Production.ProductListPriceHistory l
     WHERE l.ProductID = p.ProductID
       AND GETDATE() BETWEEN l.StartDate AND ISNULL(l.EndDate,'9999-12-31')
     ORDER BY l.StartDate DESC
  ), p.ListPrice) AS ListPrice,
  -- RowHash over all business attributes we track for SCD2
  CONVERT(VARCHAR(64),
    HASHBYTES('SHA2_256',
      CONCAT(
        p.Name,'|', pc.Name,'|', psc.Name,'|',
        ISNULL(p.Color,''),'|', ISNULL(p.Size,''),'|',
        ISNULL(pm.Name,''),'|',
        FORMAT(p.StandardCost,'N6'),'|', FORMAT(ISNULL((
            SELECT TOP 1 l.ListPrice
            FROM Production.ProductListPriceHistory l
            WHERE l.ProductID = p.ProductID
              AND GETDATE() BETWEEN l.StartDate AND ISNULL(l.EndDate,'9999-12-31')
            ORDER BY l.StartDate DESC
        ), p.ListPrice),'N6')
      )
    ), 2) AS RowHash
